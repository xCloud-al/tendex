{
  "kind": "collectionType",
  "collectionName": "offers",
  "info": {
    "singularName": "offer",
    "pluralName": "offers",
    "displayName": "Offer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "offer_status": {
      "type": "enumeration",
      "enum": [
        "DRAFT",
        "SUBMITTED"
      ]
    },
    "submitted_at": {
      "type": "datetime"
    },
    "documents": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "evaluations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::evaluation.evaluation",
      "mappedBy": "offer"
    },
    "tender": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tender.tender",
      "inversedBy": "offers"
    },
    "vendor": {
      "type": "component",
      "repeatable": false,
      "component": "offer.vendor"
    },
    "final_score": {
      "type": "integer"
    },
    "automatic_evaluation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::automatic-evaluation.automatic-evaluation",
      "mappedBy": "offer"
    }
  }
}
